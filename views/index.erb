<table>
  <thead>
    <tr>
      <th width="200">
          Recipes
      </th>
    </tr>
  </thead>
  <tbody>
    <% @recipes.to_a.each do |recipe| %>
    <tr>
      <td><a href="/recipes/<%= recipe["id"] %>">
        <%= recipe["name"] %>
        </a>
      </td>
    <% end %>
  </tbody>
</table>
